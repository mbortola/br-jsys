	<scoperte data="13/5/2008">
		<titolo>Select distinct in XSL</titolo>
		
		<descrizione>
			&lt;xsl:key name=&quot;padreID&quot; match=&quot;//FIELD[@NAME=&apos;padre&apos;]&quot; use=&quot;.&quot;/&gt;
			&lt;xsl:for-each select=&quot;//RECORD/FIELD[@NAME=&apos;padre&apos; and generate-id()=generate-id(key(&apos;padreID&apos;,.))]&quot;/ &gt;
		</descrizione>
	</scoperte>
	<scoperte>
		<titolo>selezione elementi senza testo</titolo>
		<descrizione>&lt;xsl:for-each select=&quot;//RECORD[not(string(/FIELD[@NAME=&apos;padre&apos;]))]&quot;&gt;</descrizione>
		</scoperte>
